@using SeyahatGeziWebProje.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var x in Model) { 
                <img src="@x.FotoUrl"  style="height:400px; width:650px;" alt="" />
                <p> @x.Aciklama </p>
            }
        </div>
    </div>
</div>


